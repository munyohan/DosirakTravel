<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dosirak.web.dao.CityDAO">

	<select id="getCity" resultType="com.dosirak.web.vo.City">
		SELECT * FROM CITY WHERE CITY = #{city}
	</select>

	<select id="getCityList" resultType="com.dosirak.web.vo.City">
		SELECT * FROM CITY
	</select>

</mapper>