<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dosirak.web.dao.PictureDAO">

	<select id="getPicture" resultType="com.dosirak.web.vo.Picture">
		SELECT * FROM PICTURE WHERE PCODE = #{pCode}
	</select>
	
	<select id="getPictureList" resultType="com.dosirak.web.vo.Picture">
		SELECT * FROM PICTURE
	</select>
	
</mapper>